<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.DonationInfoController">
    <children>
        <ImageView accessibleRole="TEXT_AREA" blendMode="OVERLAY" fitHeight="600.0" fitWidth="1000.0" layoutY="-1.0" pickOnBounds="true">
            <image>
                <Image url="@images/Presentation1_Proposal.jpg" />
            </image>
        </ImageView>
        <TableView fx:id="DonationInfoTable" layoutX="28.0" layoutY="123.0" prefHeight="458.0" prefWidth="640.0">
            <columns>
                <TableColumn fx:id="col_serial" prefWidth="69.60003662109375" resizable="false" text="Serial" />
                <TableColumn fx:id="col_name" prefWidth="161.60003662109375" resizable="false" text="Donee Name" />
                <TableColumn fx:id="col_location" prefWidth="116.0" resizable="false" text="Location" />
                <TableColumn fx:id="col_date" prefWidth="133.5999755859375" resizable="false" text="Date of Donation" />
                <TableColumn fx:id="col_details" minWidth="18.0" resizable="false" text="Details" />
                <TableColumn fx:id="col_update" minWidth="18.0" prefWidth="76.79998779296875" resizable="false" text="Update" />
            </columns>
        </TableView>
        <Button fx:id="back_button" layoutX="27.0" layoutY="25.0" mnemonicParsing="false" onAction="#pressBack" prefHeight="42.0" prefWidth="69.0" text="Back" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="18.0" />
            </font>
        </Button>
        <Label fx:id="title_label" layoutX="227.0" prefHeight="68.0" prefWidth="546.0" text="Donation Information" textAlignment="JUSTIFY" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="54.0" />
            </font>
        </Label>
        <Label layoutX="158.0" layoutY="84.0" text="Username: ">
            <font>
                <Font name="Cambria Bold" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="txtUser" editable="false" layoutX="250.0" layoutY="81.0" />
      <Button fx:id="newDonation" layoutX="510.0" layoutY="76.0" mnemonicParsing="false" onAction="#pressNewDonation" prefHeight="34.0" prefWidth="149.0" text="Add New Donation" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="15.0" />
         </font>
      </Button>
      <Label layoutX="810.0" layoutY="115.0" text="Filters" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="23.0" />
         </font>
      </Label>
      <Label layoutX="686.0" layoutY="160.0" text="Starting Date" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
      </Label>
      <DatePicker fx:id="startingDate" layoutX="803.0" layoutY="157.0" />
      <Label layoutX="686.0" layoutY="197.0" text="Ending Date" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
      </Label>
      <DatePicker fx:id="endingDate" layoutX="803.0" layoutY="194.0" />
      <Label layoutX="689.0" layoutY="241.0" text="Location" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="16.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="location_info" layoutX="803.0" layoutY="238.0" prefHeight="26.0" prefWidth="174.0" />
      <Button fx:id="searchButton" layoutX="879.0" layoutY="331.0" mnemonicParsing="false" onAction="#pressSearch" prefHeight="42.0" prefWidth="83.0" text="Search" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="resetButton" layoutX="783.0" layoutY="331.0" mnemonicParsing="false" onAction="#pressReset" prefHeight="42.0" prefWidth="83.0" text="Reset" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Button>
    </children>

</AnchorPane>
