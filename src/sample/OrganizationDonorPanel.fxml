<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrganizationDonorPanelController">
    <children>
        <ImageView accessibleRole="TEXT_AREA" blendMode="OVERLAY" fitHeight="600.0" fitWidth="1000.0" layoutY="-1.0" pickOnBounds="true">
            <image>
                <Image url="@images/Presentation1_Proposal.jpg" />
            </image>
        </ImageView>
        <Button fx:id="back_button" layoutX="27.0" layoutY="25.0" mnemonicParsing="false" onAction="#pressBack" prefHeight="42.0" prefWidth="69.0" text="Back" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="18.0" />
            </font>
        </Button>
        <Label fx:id="title_label" alignment="CENTER" layoutX="265.0" layoutY="14.0" prefHeight="94.0" prefWidth="538.0" text="Organization Panel" textAlignment="JUSTIFY" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="54.0" />
            </font>
        </Label>
        <Button fx:id="donpanel" layoutX="154.0" layoutY="248.0" mnemonicParsing="false" onAction="#pressOrgInfo" prefHeight="68.0" prefWidth="311.0" styleClass="panel_button" text="Organization Information" textAlignment="CENTER" textFill="#c70000" wrapText="true">
            <font>
                <Font size="22.0" />
            </font>
        </Button>
        <Button fx:id="donpanel" layoutX="154.0" layoutY="366.0" mnemonicParsing="false" onAction="#pressPendingRequest" prefHeight="68.0" prefWidth="311.0" styleClass="panel_button" text="See Pending Requests" textAlignment="CENTER" textFill="#c70000" wrapText="true">
            <font>
                <Font size="23.0" />
            </font>
        </Button>
      <ImageView fitHeight="226.0" fitWidth="262.0" layoutX="634.0" layoutY="230.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/orgpanel.png" />
         </image>
      </ImageView>
    </children>

</AnchorPane>
