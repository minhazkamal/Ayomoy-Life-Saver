<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.OrganizationInfoController">
    <children>
        <ImageView accessibleRole="TEXT_AREA" blendMode="OVERLAY" fitHeight="600.0" fitWidth="1000.0" layoutY="-1.0" pickOnBounds="true">
            <image>
                <Image url="@images/Presentation1_Proposal.jpg" />
            </image>
        </ImageView>
        <Button fx:id="back_button" layoutX="27.0" layoutY="25.0" mnemonicParsing="false" onAction="#pressBack" prefHeight="42.0" prefWidth="69.0" text="Back" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="18.0" />
            </font>
        </Button>
        <Label fx:id="title_label" layoutX="219.0" layoutY="5.0" prefHeight="82.0" prefWidth="641.0" text="Organization Information" textAlignment="JUSTIFY" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="54.0" />
            </font>
        </Label>
        <Label layoutX="169.0" layoutY="136.0" text="Username" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="19.0" />
            </font>
        </Label>
        <TextField fx:id="txtUsername" editable="false" layoutX="272.0" layoutY="135.0" prefHeight="26.0" prefWidth="174.0" />
        <Label layoutX="595.0" layoutY="136.0" text="Email" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="19.0" />
            </font>
        </Label>
        <TextField fx:id="txtEmail" layoutX="672.0" layoutY="135.0" prefHeight="26.0" prefWidth="174.0" promptText="abc@example.com" />
        <Label layoutX="92.0" layoutY="201.0" prefHeight="26.0" prefWidth="174.0" text="Organization Name" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="19.0" />
            </font>
        </Label>
        <TextField fx:id="OrgName" layoutX="272.0" layoutY="200.0" prefHeight="26.0" prefWidth="174.0" promptText="Enter organization name" />
        <Label layoutX="550.0" layoutY="185.0" text="Contact No." textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="19.0" />
            </font>
        </Label>
        <TextField fx:id="ContactNo" layoutX="672.0" layoutY="184.0" prefHeight="26.0" prefWidth="174.0" promptText="Enter mobile number" />
        <Label layoutX="180.0" layoutY="246.0" text="Location" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="19.0" />
            </font>
        </Label>
        <ChoiceBox fx:id="AddressLocation" layoutX="272.0" layoutY="245.0" prefHeight="26.0" prefWidth="174.0" />
      <Label layoutX="551.0" layoutY="230.0" text="License No." textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="LicNo" layoutX="672.0" layoutY="228.0" prefHeight="26.0" prefWidth="174.0" promptText="Enter License Number" />
      <Label layoutX="127.0" layoutY="294.0" prefHeight="26.0" prefWidth="134.0" text="Contact Person" textFill="#c70000">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Label>
      <TextField fx:id="ContactPerson" layoutX="272.0" layoutY="294.0" prefHeight="26.0" prefWidth="174.0" promptText="Enter your full name" />
      <Label layoutX="551.0" layoutY="274.0" prefHeight="53.0" prefWidth="106.0" text="Emergency &#10;Mobile No." textFill="#c70000">
