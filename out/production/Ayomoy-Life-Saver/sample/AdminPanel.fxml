<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminPanelController">
    <children>
        <ImageView accessibleRole="TEXT_AREA" blendMode="OVERLAY" fitHeight="600.0" fitWidth="1000.0" layoutX="-83.0" pickOnBounds="true">
            <image>
                <Image url="@images/Presentation1_Proposal.jpg" />
            </image>
        </ImageView>
        <Button fx:id="back_button" layoutX="14.0" layoutY="25.0" mnemonicParsing="false" onAction="#pressBack" prefHeight="42.0" prefWidth="89.0" text="Log Out" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="18.0" />
            </font>
        </Button>
        <Label fx:id="title_label" layoutX="318.0" layoutY="14.0" prefHeight="82.0" prefWidth="362.0" text="ADMIN PANEL" textAlignment="JUSTIFY" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="54.0" />
            </font>
        </Label>
        <Button fx:id="changePass" layoutX="500.0" layoutY="86.0" mnemonicParsing="false" onAction="#pressChangePass" prefHeight="23.0" prefWidth="174.0" text="Change Password" textFill="#c70000">
            <font>
                <Font name="Cambria Bold" size="18.0" />
            </font>
        </Button>
        <Button layoutX="324.0" layoutY="191.0" mnemonicParsing="false" onAction="#pressDonorList" prefHeight="118.0" prefWidth="352.0" text="Donor's List" textFill="#c70000">
         <font>
            <Font size="38.0" />
         </font>
        </Button>
        <Button layoutX="324.0" layoutY="358.0" mnemonicParsing="false" onAction="#pressOrgList" prefHeight="118.0" prefWidth="352.0" text="Organization's List" textFill="#c70000">
            <font>
                <Font name="Corbel Bold" size="35.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
