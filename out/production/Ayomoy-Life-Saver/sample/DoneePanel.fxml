      </Button>
      <Button fx:id="SearchButton" layoutX="324.0" layoutY="433.0" mnemonicParsing="false" onAction="#pressSearch" opacity="0.15" prefHeight="118.0" prefWidth="352.0" text="NewReq" textFill="#c70000">
         <font>
            <Font size="1.0" />
         </font>
      </Button>
